<% @employee.each do |f| %>
<% if f.last_working_day.present? && f.last_working_day < Time.now %>
  <%= f.last_working_day %>
<% end %>
 <% if f.user.present? &&  f.user.block_access.blank? %>
 <td><%= link_to 'Block Access', block_access_employee_path(f.id) %></td>
 <% elsif %>
 <td><%= link_to 'Enable Access', allow_access_employee_path(f.id) %></td>
 <% else %>
          <td>&nbsp;&nbsp;</td>
<% end %>
<%= f.id %>
<%= f.name %>
<%= f.dob %>
<%= f.join_date %>
<%= f.phone %>
<%= f.company.try(:name) %>
<%= f.department.try(:name) %>
<%= f.position.try(:name) %>
<%= f.position.try(:repoter).try(:name) %>
<%= f.custom_repoter.try(:name)%>
    <% if f.user.present? %>
    <td>&nbsp;&nbsp;</td>
    <% else %>
    <td><%= link_to 'Access', new_access_employee_path(f.id) %></td>
    <% end %>
<%= link_to 'edit', edit_employee_path(f.id) %>
<br>
<% end %>

<!-- Button trigger modal -->
<button type="button" class="btn btn-primary" data-toggle="modal" data-target="#exampleModal">
  Launch demo modal
</button>

<!-- Modal -->
<div class="modal fade" id="exampleModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="exampleModalLabel">Modal title</h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body">
        ...
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
        <button type="button" class="btn btn-primary">Save changes</button>
      </div>
    </div>
  </div>
</div>